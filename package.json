{
  "name": "python-editor",
  "version": "0.1.0-local",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@chakra-ui/icons": "^1.0.15",
    "@chakra-ui/react": "^1.6.7",
    "@codemirror/autocomplete": "~0",
    "@codemirror/closebrackets": "~0",
    "@codemirror/commands": "~0",
    "@codemirror/comment": "~0",
    "@codemirror/fold": "~0",
    "@codemirror/gutter": "~0",
    "@codemirror/highlight": "~0",
    "@codemirror/history": "~0",
    "@codemirror/lang-python": "~0",
    "@codemirror/language": "~0",
    "@codemirror/lint": "~0",
    "@codemirror/state": "~0",
    "@codemirror/view": "~0",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.1.5",
    "@microbit/microbit-fs": "^0.9.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "@types/dompurify": "^2.3.1",
    "@types/lodash.sortby": "^4.7.6",
    "@types/marked": "^3.0.1",
    "@types/node": "^14.17.15",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "base64-js": "^1.5.1",
    "dapjs": "2.2.0",
    "dompurify": "^2.3.3",
    "file-saver": "^2.0.5",
    "framer-motion": "^3.10.6",
    "lodash.sortby": "^4.7.0",
    "lzma": "^2.3.2",
    "marked": "^3.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-intl": "^5.20.10",
    "vscode-jsonrpc": "^6.0.0",
    "vscode-languageserver-protocol": "^3.16.0",
    "web-vitals": "^1.1.1",
    "xterm": "4.14.1",
    "xterm-addon-fit": "^0.5.0"
  },
  "devDependencies": {
    "@craco/craco": "^6.2.0",
    "@formatjs/cli": "^4.2.33",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "@types/file-saver": "^2.0.3",
    "@types/jest": "^26.0.24",
    "pptr-testing-library": "^0.6.4",
    "prettier": "2.3.2",
    "puppeteer": "^8.0.0",
    "react-scripts": "4.0.3",
    "source-map-explorer": "^2.5.2",
    "typescript": "^4.4.2",
    "webusb": "^2.2.0"
  },
  "scripts": {
    "bundle-size-explorer": "source-map-explorer 'build/static/js/*.js'",
    "bundle-size-report": "source-map-explorer 'build/static/js/*.js' --html reports/bundle-size.html",
    "ci": "npm run test && npm run build && npm run bundle-size-report",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test --testPathIgnorePatterns=e2e",
    "eject": "craco eject",
    "serve": "npx serve --no-clipboard -l 3000 -- build/",
    "test:e2e": "craco test --testPathPattern e2e -w 1 --testTimeout 15000",
    "test:e2e:headless": "CI=true npm run test:e2e",
    "ci:update-version": "update-ci-version",
    "deploy": "website-deploy-aws",
    "invalidate": "aws cloudfront create-invalidation --distribution-id $(printenv ${STAGE}_CLOUDFRONT_DISTRIBUTION_ID) --paths \"/*\"",
    "i18n:compile": "node bin/sort.js && formatjs compile-folder --ast lang src/messages",
    "fix-licensing-headers": "node bin/fix-licensing-headers.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
